<?xml version="1.0" encoding="UTF-8"?>
<SearchResult>
<#list resultSet as article>
  <Article>
    <Title><#if article.title?has_content>${article.title}</#if></Title>
    <Author><#if article.author?has_content>${article.author}</#if></Author>
    <Location><#if article.location?has_content>${article.location}</#if></Location>
    <Teaser><#if article.teaser?has_content>${article.teaser}</#if></Teaser>
    <Body><#if article.body?has_content>${article.body}</#if></Body>
    <LaunchDate><#if article.launch_date?has_content>${article.launch_date}</#if></LaunchDate>
  </Article>
</#list>
</SearchResult>
