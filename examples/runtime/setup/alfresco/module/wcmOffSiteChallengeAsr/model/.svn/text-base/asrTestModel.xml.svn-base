<?xml version="1.0" encoding="UTF-8"?>

<!-- ASR Off Site Challenge test Content Model for XML Metadata Extraction -->
<model name="osc:nraModel" xmlns="http://www.alfresco.org/model/dictionary/1.0">

    <description>Off Site Challenge Custom Content Model</description>
    <version>1.0</version>

    <imports>
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
        <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
    </imports>

    <namespaces>
        <namespace uri="http://www.alfresco.org/alfresco/osc/contentmodel" prefix="osc"/>
    </namespaces>

    <aspects>
        <aspect name="osc:article">
            <title>Article properties</title>
            <properties>
                <property name="osc:articletitle">
                    <type>d:text</type>
                    <multiple>true</multiple>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>true</tokenised>
                    </index>
                </property>
                <property name="osc:articleauthor">
                    <type>d:text</type>
                    <multiple>false</multiple>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>false</tokenised>
                    </index>
                </property>
                <property name="osc:articlelocation">
                    <type>d:text</type>
                    <multiple>false</multiple>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>false</tokenised>
                    </index>
                </property>
                <property name="osc:articleteaser">
                    <type>d:text</type>
                    <multiple>true</multiple>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>true</tokenised>
                    </index>
                </property>
                <property name="osc:articlebody">
                    <type>d:text</type>
                    <multiple>true</multiple>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>true</tokenised>
                    </index>
                </property>
                <property name="osc:articlelaunchdate">
                    <type>d:date</type>
                    <multiple>false</multiple>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>false</tokenised>
                    </index>
                </property>
            </properties>
        </aspect>
        <aspect name="osc:newsflash">
            <title>News Flash Properties</title>
            <properties>
                <property name="osc:flashtitle">
                    <type>d:text</type>
                    <multiple>true</multiple>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>true</tokenised>
                    </index>
                </property>
                <property name="osc:flashbody">
                    <type>d:text</type>
                    <multiple>true</multiple>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>true</tokenised>
                    </index>
                </property>
                <property name="osc:flashlaunchdate">
                    <type>d:date</type>
                    <multiple>false</multiple>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>false</tokenised>
                    </index>
                </property>
            </properties>
        </aspect>
    </aspects>

</model>
